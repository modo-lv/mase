<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="BOTTOM_RIGHT" prefHeight="198.0" prefWidth="348.0" stylesheets="@style.css"
      xmlns="http://javafx.com/javafx/23.0.1"
      xmlns:fx="http://javafx.com/fxml/1">
  <HBox styleClass="padded">
    <Label text="&lt;attribute&gt;">
      <font>
        <Font size="24.0"/>
      </font>
    </Label>
  </HBox>
  <Region prefHeight="200.0" prefWidth="200.0"/>
  <ButtonBar prefHeight="40.0" prefWidth="200.0" styleClass="padded">
    <buttons>
      <Button cancelButton="true" text="Cancel"/>
      <Button defaultButton="true" text="OK"/>
    </buttons>
    <opaqueInsets>
      <Insets/>
    </opaqueInsets>
  </ButtonBar>
</VBox>
